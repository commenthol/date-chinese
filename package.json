{
  "name": "date-chinese",
  "version": "1.0.1",
  "description": "Chinese Calendar",
  "main": "lib",
  "jsnext:main": "src",
  "engines": {
    "node": ">=0.12.0"
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "mocha --compilers js:babel-register --check-leaks test/*.mocha.js",
    "cover": "istanbul cover _mocha -- test/*.mocha.js --reporter dot ",
    "lint": "eslint --quiet '**/*.js'",
    "transpile": "babel src --out-dir lib",
    "dev": "babel --watch src --out-dir lib"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/commenthol/date-chinese.git"
  },
  "keywords": [
    "chinese",
    "calendar",
    "gregorian",
    "conversion",
    "chinese new year",
    "zhongqi",
    "jieqi",
    "qingming",
    "korean",
    "vietnamese",
    "japanese",
    "sekki"
  ],
  "author": "commenthol <commenthol@gmail.com>",
  "maintainers": "commenthol <commenthol@gmail.com>",
  "dependencies": {
    "astronomia": "^1.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.16.0",
    "babel-preset-es2015": "^6.16.0",
    "babel-register": "^6.16.3",
    "mocha": "^3.1.2"
  },
  "optionalDependencies": {
    "eslint": "^3.7.0",
    "eslint-config-standard": "^6.2.0",
    "eslint-plugin-promise": "^3.3.0",
    "eslint-plugin-standard": "^2.0.1"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "eslintConfig": {
    "extends": "standard"
  }
}
